d_hv <- c(0,voda$hv_v[2:length(voda$hv_v)] - voda$hv_v[1:length(voda$hv_v)-1] +
voda$hv_k[2:length(voda$hv_k)] - voda$hv_k[1:length(voda$hv_k)-1])
d_gv <- c(0,voda$gv_v[2:length(voda$gv_v)] - voda$gv_v[1:length(voda$gv_v)-1] +
voda$gv_k[2:length(voda$gv_k)] - voda$gv_k[1:length(voda$gv_k)-1])
voda <- mutate(voda,
d_hv,
d_gv,
'холодная вода' = tarif$hv*d_hv,
'отвод хол.воды' = tarif$hv_otv*d_hv,
'отвод гор.воды' = tarif$hv_otv*d_gv,
'всего' = tarif$hv*d_hv + tarif$hv_otv*d_hv + tarif$hv_otv*d_gv)
